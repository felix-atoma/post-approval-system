{
  "name": "post-management-api",
  "version": "1.0.0",
  "description": "Backend API for post management system",
  "main": "src/server.js",
  "scripts": {
  "start": "node src/server.js",
  "dev": "nodemon src/server.js",
  "test": "jest --coverage --coverageThreshold='{}'",
  "test:watch": "jest --watch",
  "prisma:generate": "prisma generate",
  "prisma:migrate": "prisma migrate dev",
  "prisma:seed": "node prisma/seed.js",
  "prisma:studio": "prisma studio",
  "lint": "eslint src/**/*.js",
  "format": "prettier --write src/**/*.js"
},

  "keywords": ["express", "node", "prisma", "jwt", "postgresql"],
  "author": "",
  "license": "MIT",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}